{
    "greet": {
        "desc": "Basic greetings.",
        "alias": ["hi", "hello"],
        "usage": "$greet"
    },
    "gao": {
        "desc": "Make a Botan-style roar!",
        "alias": [],
        "usage": "$gao"
    },
    "debut": {
        "desc": "Check time left until debut.",
        "alias": [],
        "usage": "$debut"
    },
    "birthday": {
        "desc": "Find out about Botan's birthday.",
        "alias": ["bday"],
        "usage": "$birthday"
    },
    "translate": {
        "desc": "Translate any text to english.",
        "alias": ["trans"],
        "usage": "$translate {text}"
    },
    "japanese": {
        "desc": "Translate any text to japanese with pronunciation.",
        "alias": ["jp"],
        "usage": "$japanese {text}"
    },
    "meme": {
        "desc": "Generate customized meme contents.\n\n__Available memes:__\nwoke\nwoken\nfade\ndisgust\ntestimony",
        "alias": [],
        "usage": "$meme {meme}\n{first argument}\n{second argument}\n..."
    },
    "botan": {
        "desc": "Randomly displays an artwork of Botan-sama.",
        "alias": [],
        "usage": "$botan"
    },
    "100": {
        "desc": "Botan scores you with a 100!",
        "alias": [],
        "usage": "$100"
    },
    "sleepy": {
        "desc": "Find out how many times are Bodans sleepy so far.",
        "alias": [],
        "usage": "$sleepy"
    },
    "subscribers": {
        "desc": "Check subscribers count of Botan's or other vtubers' YouTube channel.",
        "alias": ["subs", "subscriber"],
        "usage": "$subscribers {optional:name}"
    },
    "stream": {
        "desc": "Check Botan's or other vtubers' streaming status on YouTube.",
        "alias": ["live"],
        "usage": "$stream {optional:name}"
    },
    "voice": {
        "desc": "Sends cute Botan voice clips.\n\n**In Stock:**\ndoya\npoi\nyes\nyay\nok\narigatou\nhyaaa\nty",
        "alias": ["v"],
        "usage": "$voice {optional:clip name}"
    },
    "superchat": {
        "desc": "Create a customized superchat image.",
        "alias": ["sc"],
        "usage": "$superchat {money amount}\n{text}"
    }
}